    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "^2.9.3",
    "express": "^4.18.2",
    "firebase": "^10.7.2",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "mongoose": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-range-slider-expo": "^1.4.3",
    "react-native-reanimated": "^3.6.1",
